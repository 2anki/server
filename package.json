{
  "name": "notion2anki",
  "description": "Go from Notion togglelists to Anki Cards easily!",
  "keywords": [
    "notion",
    "anki",
    "notion2anki",
    "imba"
  ],
  "author": "Alexander Alemayhu",
  "version": "0.2.45",
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "main.js",
  "scripts": {
    "dev": "webpack-dev-server --mode=development --content-base dist/",
    "watch": "webpack --watch --progress --colors --display-modules",
    "test": "ava",
    "build": "imbac server && webpack --production",
    "compile-server": "imbac server",
    "start": "yarn dev",
    "dev-server": "nodemon --exec yarn run imba server/server.imba"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alemayhu/notion2anki.git"
  },
  "license": "MIT",
  "dependencies": {
    "cheerio": "1.0.0-rc.3",
    "express": "^4.17.1",
    "imba": "2.0.0-alpha.100",
    "jszip": "^3.5.0",
    "knex": "^0.21.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.12",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.7.1"
  },
  "build": {
    "appId": "com.alemayhu.notion2anki",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "main.js",
      "dist/app.js",
      "dist/index.html",
      "build",
      "node_modules"
    ]
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "nodemon": "^2.0.7"
  }
}
