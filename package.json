    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.7.0"
  }
